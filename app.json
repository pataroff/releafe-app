{
  "expo": {
    "name": "releafe-app",
    "slug": "releafe-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.expo.releafeapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "dev.expo.releafeapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SofiaProLight.ttf",
            "./assets/fonts/SofiaProRegular.ttf",
            "./assets/fonts/SofiaProRegular-Italic.ttf",
            "./assets/fonts/SofiaProMedium.ttf",
            "./assets/fonts/SofiaProMedium-Italic.ttf",
            "./assets/fonts/SofiaProSemiBold.ttf",
            "./assets/fonts/SofiaProBold.ttf",
            "./assets/fonts/SofiaProBlack.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Releafe to access your camera",
          "microphonePermission": "Allow Releafe to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Releafe to access your microphone."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-audio",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "2379856a-8b9f-46a2-8e1c-0d5b50e0169b"
      }
    },
    "owner": "iwwan_01"
  }
}
